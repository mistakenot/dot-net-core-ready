<body>
    <nav class="navbar color-accent theme-dark" style="min-height: 12px">
        <div class="navbar-local color-accent theme-dark">
            <div class="container">
                <div _class="collapse navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li>
                            <a href="//blog.dotnetcoreready.com/" style="padding: 6px 6px">Blog</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    
    <div class="alert-stack">
        <div id="email-alert-msg" class="alert alert-warning alert-dismissible fade in" role="alert" hidden >
            <button id="email-alert-msg-btn" type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true"><i class="glyph glyph-cancel"></i></span>
            </button>

            <div class="container">
                <div class="row">
                    <div class="col-md-24 text-center">
                        <p>Email alert created successfully.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="jumbotron theme-light">
        <div class="container">
            <div class="row">
                <div class="col-xs-24 text-center">
                    <div class="visible-sm visible-md visible-lg">
                        <h1 style="padding-top: 2px">.NET Core Ready</h1>
                        <div class="type-sh3">
                            Check whether your favourite NuGet packages are ready for .NET Core.
                        </div>
                    </div>
                    <div class="visible-xs">
                        <h2 style="padding-top: 2px">.NET Core Ready</h2>
                        <div class="type-sh6">
                            Check whether your favourite NuGet packages are ready for .NET Core.
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

        <br/>

        <div class="container body-content">
            @RenderBody()
            <hr/>
            <footer>
                <div class="row">
                    <div class="col-xs-24">
                        <div class="text-center">
                            <br/>
                            <div class="addthis_inline_share_toolbox"></div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-xs-24">
                        <p class="text-center">
                            Created by <a href="https://twitter.com/jazzyskeltor">@@jazzyskeltor</a> on <a href="https://github.com/mistakenot/dot-net-core-ready">Github</a>.
                        </p>
                    </div>
                </div>
            </footer>
        </div>

    <script src="js/site.min.js"></script>
    @Html.Partial("_Components")
    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-59ca7217e33ad650"></script>
